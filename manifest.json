{
    "manifest_version": 3,
    "name": "Archive Any Website Today",
    "version": "2.5",
    "description": "Publicly archive and preserve any public webpage using the archive.today, archive.is, archive.ph, archive.vn service",
    "permissions": [
      "tabs", "contextMenus"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
      },
      "default_title": "Archive Webpage"
    },
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  }
  